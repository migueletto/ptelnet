END

FORM SerialForm 2 2 156 142
USABLE
MODAL
SAVEBEHIND
DEFAULTBTNID cancelBtn
BEGIN
  TITLE "Serial options" 
  LABEL "Port:" 3005 4 16 FONT 1
  POPUPTRIGGER "Serial" portCtl 58 PREVTOP AUTO AUTO LEFTANCHOR
  LIST "port1description" "port2description" portList PREVLEFT+4 PREVTOP AUTO AUTO VISIBLEITEMS 2 NONUSABLE
  POPUPLIST portCtl portList
  LABEL "Baud:" 3001 4 33 FONT 1
  POPUPTRIGGER "9600" baudCtl 58 PREVTOP AUTO AUTO LEFTANCHOR
  LIST "300" "1200" "2400" "4800" "9600" "19200" "38400" "57600" "115200" baudList PREVLEFT+4 PREVTOP AUTO AUTO VISIBLEITEMS 9 NONUSABLE
  POPUPLIST baudCtl baudList
  LABEL "Parity:" 3002 4 50 FONT 1
  PUSHBUTTON "N" paritySelNone 58 PREVTOP AUTO AUTO GROUP 1
  PUSHBUTTON "E" paritySelEven PREVRIGHT+1 PREVTOP PREVWIDTH PREVHEIGHT GROUP 1
  PUSHBUTTON "O" paritySelOdd PREVRIGHT+1 PREVTOP PREVWIDTH PREVHEIGHT GROUP 1
  LABEL "Word:" 3003 4 PREVBOTTOM+6 FONT 1
  PUSHBUTTON "5" bitsSel5 58 PREVTOP AUTO AUTO GROUP 2
  PUSHBUTTON "6" bitsSel6 PREVRIGHT+1 PREVTOP PREVWIDTH PREVHEIGHT GROUP 2
  PUSHBUTTON "7" bitsSel7 PREVRIGHT+1 PREVTOP PREVWIDTH PREVHEIGHT GROUP 2
  PUSHBUTTON "8" bitsSel8 PREVRIGHT+1 PREVTOP PREVWIDTH PREVHEIGHT GROUP 2
  LABEL "StopBits:" 3004 4 PREVBOTTOM+6 FONT 1
  PUSHBUTTON "1" stopBitsSel1 58 PREVTOP AUTO AUTO GROUP 3
  PUSHBUTTON "2" stopBitsSel2 PREVRIGHT+1 PREVTOP PREVWIDTH PREVHEIGHT GROUP 3
  CHECKBOX "Xon/Xoff" xonXoffCtl 4 PREVBOTTOM+6 AUTO AUTO FONT 1
  CHECKBOX "RTS/CTS" rtsCtsCtl PREVRIGHT+10 PREVTOP AUTO AUTO FONT 1
  BUTTON "Ok" okBtn 8 PREVBOTTOM+8 AUTO AUTO
  BUTTON "Cancel" cancelBtn PREVRIGHT+5 PREVTOP AUTO AUTO
END

