  PUSHBUTTON "CR" crSel 48 PREVTOP AUTO AUTO GROUP 1
  PUSHBUTTON "LF" lfSel PREVRIGHT+1 PREVTOP PREVWIDTH PREVHEIGHT GROUP 1
  PUSHBUTTON "CR/LF" crlfSel PREVRIGHT+1 PREVTOP 2*PREVWIDTH+1 PREVHEIGHT GROUP 1
  LABEL "Font:" 3006 4 PREVBOTTOM+6 FONT 1
  PUSHBUTTON "4x6"  sel4x6   48          PREVTOP AUTO      AUTO       GROUP 4
  PUSHBUTTON "6x6"  hsel6x6  PREVLEFT    PREVTOP PREVWIDTH PREVHEIGHT GROUP 6
  PUSHBUTTON "5x9"  sel5x9   PREVRIGHT+1 PREVTOP PREVWIDTH PREVHEIGHT GROUP 4
  PUSHBUTTON "6x10" hsel6x10 PREVLEFT    PREVTOP PREVWIDTH PREVHEIGHT GROUP 6
  PUSHBUTTON "6x6"  sel6x6   PREVRIGHT+1 PREVTOP PREVWIDTH PREVHEIGHT GROUP 4
  PUSHBUTTON "8x14" hsel8x14 PREVLEFT    PREVTOP PREVWIDTH PREVHEIGHT GROUP 6
  CHECKBOX "Local echo" echoCtl 4 PREVBOTTOM+6 AUTO AUTO FONT 1
  BUTTON "Ok" okBtn 8 PREVBOTTOM+8 AUTO AUTO
  BUTTON "Cancel" cancelBtn PREVRIGHT+5 PREVTOP AUTO AUTO
  POPUPLIST csCtl csList
END

